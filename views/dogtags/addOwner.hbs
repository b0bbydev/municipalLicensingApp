<div class="content">
  <h2 id="title">Add Owner</h2>
  <form class="row g-2" id="addLicenseForm" method="POST">
    <h4>Owner Information</h4>
    <hr />
    <div class="row">
      <!-- First Name -->
      <div class="col-sm-6">
        <label for="First Name" class="form-label">First Name</label>
        <input
          type="text"
          placeholder="First Name"
          class="form-control"
          name="firstName"
        />
      </div>
      <!-- Last Name -->
      <div class="col-sm-6">
        <label for="Last Name" class="form-label">Last Name</label>
        <input
          type="text"
          placeholder="Last Name"
          class="form-control"
          name="lastName"
        />
      </div>
    </div>
    <div class="row">
      <!-- PO Box/Apt/RR -->
      <div class="col-sm-6">
        <label for="PO Box/Apt/RR " class="form-label">PO Box/Apt/RR</label>
        <input
          type="text"
          class="form-control"
          placeholder="PO Box/Apt/RR"
          name="poBoxAptRR"
        />
      </div>
      <!-- Address -->
      <div class="col-sm-6">
        <label for="Address" class="form-label">Address</label>
        <input
          type="text"
          class="form-control"
          placeholder="Address"
          name="address"
        />
      </div>
    </div>
    <div class="row">
      <!-- Town -->
      <div class="col-sm-6">
        <label for="Town" class="form-label">Town</label>
        <input
          type="text"
          class="form-control"
          placeholder="Town"
          name="town"
        />
      </div>
      <!-- Postal Code -->
      <div class="col-sm-6">
        <label for="Postal Code" class="form-label">Postal Code</label>
        <input
          type="text"
          class="form-control"
          placeholder="Postal Code"
          name="postalCode"
        />
      </div>
    </div>
    <div class="row">
      <!-- Home Phone -->
      <div class="col-sm-4">
        <label for="Home Phone" class="form-label">Home Phone</label>
        <input
          type="text"
          class="form-control"
          placeholder="Home Phone"
          name="homePhone"
        />
      </div>
      <!-- Cell Phone -->
      <div class="col-sm-4">
        <label for="Cell Phone" class="form-label">Cell Phone</label>
        <input
          type="text"
          class="form-control"
          placeholder="Cell Phone"
          name="cellPhone"
        />
      </div>
      <!-- Work Phone -->
      <div class="col-sm-4">
        <label for="Work Phone" class="form-label">Work Phone</label>
        <input
          type="text"
          class="form-control"
          placeholder="Work Phone"
          name="workPhone"
        />
      </div>
    </div>
    <!-- Email -->
    <div class="row">
      <div class="col-sm-12">
        <label for="Email" class="form-label">Email</label>
        <input
          type="text"
          class="form-control"
          placeholder="Email"
          name="email"
        />
      </div>
    </div>
    <div class="row">
      <div class="col">
        <button
          id="licenseSubmit"
          class="btn btn-primary"
          type="submit"
        >Submit</button>
      </div>
    </div>
