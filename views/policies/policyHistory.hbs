<h2>{{policyName}} - History</h2>
<div class="content">
  <section class="row">
    <div class="col-12">
      <h4>Policy History</h4>
      {{#group policyHistory by="year"}}
        <strong>{{{value}}}</strong>
        <table class="sortable table table-bordered table-striped table-hover">
          <thead>
            <tr>
              <th>Policy ID</th>
              <th>Last Modified</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            {{#each items}}
              <tr>
                <td>{{this.policyID}}</td>
                <td>{{this.lastModified}}</td>
                <td>{{this.action}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      {{/group}}
    </div>
  </section>
</div>